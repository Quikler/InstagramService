<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>InstaService</PackageId>
		<Description>A wrapper library on top of the InstagramApiSharp library. The library provides simple functionality for downloading media files from Instagram.</Description>
		<Authors>Roman Binyukov | Quikler</Authors>
		<Copyright>Copyright Â© 2024 Roman Binyukov | Quikler</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/Quikler/InstagramService</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Quikler/InstagramService</RepositoryUrl>
		<PackageTags>
			Instagram InstagramDownloader InstagramMediaDownloader
			InstagramDownload Insta InstaDownloader InstaMediaDownloader
			InstaService InstagramService InstagramMediaService
		</PackageTags>
		<PackageVersion>1.0.1</PackageVersion>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Interfaces\**" />
		<EmbeddedResource Remove="Interfaces\**" />
		<None Remove="Interfaces\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="InstagramApiSharp" Version="1.7.4" />
	</ItemGroup>

	<ItemGroup>
		<None Include="README.md">
			<Pack>true</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
